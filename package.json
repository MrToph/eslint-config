{
  "name": "eslint-config-okonet",
  "version": "1.2.0",
  "description": "ESLint rules from Andrey Okonetchnikov",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "deps": "npm-check -s",
    "deps:update": "npm-check -u"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okonet/eslint-config-okonet.git"
  },
  "author": "Andrey Okonetchnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okonet/eslint-config-okonet/issues"
  },
  "homepage": "https://github.com/okonet/eslint-config-okonet#readme",
  "dependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.3.0"
  },
  "peerDependencies": {
    "eslint": "3.x"
  },
  "devDependencies": {
    "lint-staged": "^3.2.1",
    "npm-check": "^5.0.1",
    "pre-commit": "^1.1.3"
  },
  "eslintConfig": {
    "extends": "./index.js"
  }
}
